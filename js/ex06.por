programa
{
	
	funcao inicio()
	{
		inteiro n
		escreva(" Digite um número ")
		leia(n)
		escreva(" O antecessor de " + n + " é " + (n- 1)+" , ")
		escreva(" O sucessor de " + n + " é " + (n + 1))
	}
}
